{
	"name": "install-purescript-cli",
	"version": "0.3.0",
	"description": "A command-line tool to install PureScript",
	"repository": "shinnn/install-purescript-cli",
	"author": "Shinnosuke Watanabe (https://github.com/shinnn)",
	"scripts": {
		"run-docker": "docker run --tty --volume $PWD:/project --workdir /project node node . tmp/docker",
		"pretest": "eslint --fix --format=codeframe index.js test",
		"test": "nyc --reporter=html --reporter=text ava"
	},
	"license": "ISC",
	"files": [
		"index.js"
	],
	"bin": {
		"install-purescript": "index.js"
	},
	"keywords": [
		"purescript",
		"purs",
		"binary",
		"exe",
		"executable",
		"haskell",
		"download",
		"prebuilt",
		"fallback",
		"build",
		"compile",
		"stack",
		"ghc",
		"install",
		"installation",
		"cache",
		"cross-platform",
		"command-line",
		"cli"
	],
	"dependencies": {
		"chalk": "^2.4.1",
		"install-purescript": "^0.4.0",
		"log-symbols": "^2.2.0",
		"log-update": "^2.3.0",
		"minimist": "^1.2.0",
		"ms": "^2.1.1",
		"neat-frame": "^1.0.1",
		"neat-stack": "^1.0.0",
		"once": "^1.4.0",
		"platform-name": "^1.0.0",
		"size-rate": "^0.1.0",
		"tilde-path": "^2.0.0",
		"tty-truncate": "^1.0.0",
		"vertical-meter": "^1.0.0"
	},
	"devDependencies": {
		"@shinnn/eslint-config-node": "^5.0.0",
		"app-cache-dir": "^0.3.0",
		"arch": "^2.1.0",
		"ava": "^1.0.0-beta.4",
		"different-platform": "^1.0.1",
		"eslint": "^4.19.1",
		"execa": "^0.10.0",
		"nock": "^9.2.6",
		"nyc": "^11.8.0",
		"path-exists": "^3.0.0",
		"path-key": "^2.0.1",
		"pretend-platform": "^2.0.0",
		"request": "^2.87.0",
		"rmfr": "^2.0.0",
		"strip-ansi": "^4.0.0",
		"term-size": "^1.2.0",
		"to-executable-name": "^2.0.0"
	},
	"ava": {
		"files": "test/test.js",
		"serial": true,
		"verbose": true
	},
	"eslintConfig": {
		"extends": "@shinnn/node",
		"rules": {
			"no-process-exit": "off"
		}
	}
}
