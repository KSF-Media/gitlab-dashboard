{
	"name": "feint",
	"version": "1.0.2",
	"description": "Make a given function do nothing at its first call",
	"repository": "shinnn/feint",
	"author": "Shinnosuke Watanabe (https://github.com/shinnn)",
	"scripts": {
		"prebuild": "eslint --fix --format=codeframe index.mjs test.js",
		"build": "rollup --config=node:module",
		"pretest": "npm run-script build",
		"test": "nyc --reporter=html --reporter=text node test.js"
	},
	"license": "ISC",
	"module": "index.mjs",
	"files": [
		"index.js",
		"index.mjs"
	],
	"keywords": [
		"feint",
		"function",
		"call",
		"noop",
		"time"
	],
	"dependencies": {
		"append-type": "^1.0.1"
	},
	"devDependencies": {
		"@shinnn/eslint-config": "^5.4.0",
		"eslint": "^4.18.1",
		"nyc": "^11.5.0",
		"rollup": "^0.56.3",
		"rollup-config-module": "^2.0.0",
		"tape": "^4.9.0"
	},
	"eslintConfig": {
		"extends": "@shinnn"
	}
}
